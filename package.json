{
  "name": "eslint-plugin-harlanzw",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@10.15.0",
  "description": "Harlan's opinionated ESLint rules",
  "author": "Harlan Wilton <harlan@harlanzw.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://github.com/harlan-zw/eslint-plugin-harlanzw#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/eslint-plugin-harlanzw.git"
  },
  "bugs": "https://github.com/harlan-zw/eslint-plugin-harlanzw/issues",
  "keywords": [
    "eslint-plugin"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "pnpm run dev && eslint . --fix",
    "release": "pnpm build && bumpp && pnpm -r publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@antfu/ni": "^25.0.0",
    "@antfu/utils": "^9.2.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.3.0",
    "@typescript-eslint/typescript-estree": "^8.41.0",
    "@typescript-eslint/utils": "^8.41.0",
    "bumpp": "^10.2.3",
    "eslint": "^9.34.0",
    "eslint-vitest-rule-tester": "^2.2.1",
    "jsonc-eslint-parser": "^2.4.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1",
    "vite": "^7.1.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.20"
  },
  "resolutions": {
    "eslint-plugin-harlanzw": "workspace:*"
  }
}
